{
  "presets": [
    "es2015",
    "react",
    "stage-0",
    "stage-1"
  ],
  "plugins": [
    "transform-decorators-legacy",
    "transform-class-properties",
    ["transform-es2015-classes", { "loose": true }]
  ],
  "env": {
    "development": {
      "presets": [
        "react-hmre"
      ]
    },
    "production": {
      "plugins": [
        "transform-react-constant-elements",
        "transform-react-remove-prop-types"
      ]
    }
  }
}
